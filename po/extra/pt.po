#: data/io.elementary.calendar.appdata.xml.in:42
#: data/io.elementary.calendar.appdata.xml.in:51
msgid "Minor Updates:"
msgstr "Pequenas atualizações:"

#: data/io.elementary.calendar.appdata.xml.in:26
msgid "Don't resize the sidebar when resizing the window"
msgstr "Não redimensiona a barra lateral ao redimensionar a janela"

#: data/io.elementary.calendar.appdata.xml.in:27
#: data/io.elementary.calendar.appdata.xml.in:36
