msgid "Maya;Planner;Dates;Days;Events;Calendar;ics;caldav;evolution;"
msgstr ""
"Maya;Planner;Dates;Days;Events;Calendar;ics;caldav;evolution;日付;日;予定;カ"
"レンダー;"

#: data/io.elementary.calendar.desktop.in:18
msgid "Add Event"
msgstr "予定を追加"

